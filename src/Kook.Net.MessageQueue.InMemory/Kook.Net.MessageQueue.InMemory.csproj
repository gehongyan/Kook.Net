<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="../../props/common.props"/>

  <PropertyGroup>
    <RootNamespace>Kook.Net.Queue.InMemory</RootNamespace>
    <IsPackable>true</IsPackable>
    <PackageId>Kook.Net.MessageQueue.InMemory</PackageId>
